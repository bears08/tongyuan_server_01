									url:url+"/finance/app/choose_order_quit_type",
									async:true,
									type:"POST",
									data:{act:mt,orderId:orderId},
									dataType:"JSON",
									erro:function(XMLHttpRequest,textStatus,errorThrown){
										alert("错误:状态码"+textStatus);
									},
									success:function(msg){
										if(msg.flag){
											alert("修改成功");
											console.info("修改成功");
										}else{
											alert("修改失败");
											console.info("修改失败了");
										}
									},
									statusCode:{404:function(){
										alert("未找到请求的Url");
									}}
									,
									timeout:function(){
										alert("请求发送超时");
									}
								});
							}
						</script>
